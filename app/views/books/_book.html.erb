 <%= form_with model: @book do |f| %>
   <div class="form-title row">
    <%= f.label :Title %>
    <%= f.text_field :title, class: "col-xs-12" %>
   </div>

   <div class="form-body row">
    <%= f.label :Opinion %>
    <%= f.text_area
   </div>
   <%= f.text_field :title %>
  <h4>Body</h4>
   <%= f.text_area :body %>
    <%= f.submit 'Create Book'%>
 <% end %>

 <%= paginate books %>